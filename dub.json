	"name": "icu4d",
	"dependencies": {
		"bindbc-icu": "~>74.2.0"
	},
	
	"configurations": [
		{
			"name": "static",
			"targetType": "library",
			"subConfigurations": {
				"bindbc-icu": "static"
			}
		},
		{
			"name": "static-nolib",
			"targetType": "library",
			"subConfigurations": {
				"bindbc-icu": "static-nolib"
			}
		},
		{
			"name": "dynamic",
			"targetType": "library",
			"subConfigurations": {
				"bindbc-icu": "dynamic"
			}
		},
		{
			"name": "dynamic-nolib",
			"targetType": "library",
			"subConfigurations": {
				"bindbc-icu": "dynamic-nolib"
			}
		},
		{
			"name": "both",
			"targetType": "library"
		},
		{
			"name": "unittest-debug",
			"targetType": "executable",
			"dflags": ["-main", "-mixin=.dub/mixin.d"]
		},
		{
			"name": "unittest",
			"targetType": "library",
			"dflags": ["-mixin=.dub/mixin.d"]
		}
